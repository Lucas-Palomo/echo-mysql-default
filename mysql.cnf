# Some Tweaks for Mysql Server
[mysqld]

key_buffer_size = 256M
max_allowed_packet=268435456
table_open_cache = 256
sort_buffer_size = 1M
read_buffer_size = 1M
read_rnd_buffer_size = 4M
myisam_sort_buffer_size = 64M
thread_cache_size = 8
query_cache_size= 16M
log-bin=mysql-bin
log_bin_trust_function_creators=1
binlog_format=mixed